ect gets created\n      event.preventDefault();\n      services.ProjectService.create({ data: this.newProject }).then(\n        (result) => {\n          this.$refs.modalNewProject.hide();\n          this.$emit(\"new-project\", result);\n          // Reset state\n          this.newProject = new models.Project();\n          this.$refs.projectEditor.reset();\n        }\n      );\n    },\n    onCancelNewProject() {\n      this.newProject = new models.Project();\n      this.$refs.projectEditor.reset();\n    },\n  },\n  computed: {\n    okDisabled: function () {\n      return !this.valid;\n    },\n  },\n};\n</script>\n"]}]}